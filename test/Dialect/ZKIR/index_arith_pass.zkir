// RUN: zkir-opt -split-input-file %s 2>&1 | FileCheck %s

/// Selection of index dialect that is reasonable to use in ZKIR.

func.func private @f_constant() -> index {
  %0 = index.constant 42
  return %0: index
}
//CHECK-LABEL:  func.func private @f_constant() -> index {
//CHECK-NEXT:     %[[T1:[0-9a-zA-Z_\.]+]] = index.constant 42
//CHECK-NEXT:     return %[[T1]] : index
//CHECK-NEXT:   }
// -----

func.func private @f_add(%a: index, %b: index) -> index {
  %c = index.add %a, %b
  return %c: index
}
//CHECK-LABEL:  func.func private @f_add
//CHECK-SAME:   (%[[A0:[0-9a-zA-Z_\.]+]]: index, %[[A1:[0-9a-zA-Z_\.]+]]: index) -> index {
//CHECK-NEXT:     %[[T1:[0-9a-zA-Z_\.]+]] = index.add %[[A0]], %[[A1]]
//CHECK-NEXT:     return %[[T1]] : index
//CHECK-NEXT:   }
// -----

func.func private @f_sub(%a: index, %b: index) -> index {
  %c = index.sub %a, %b
  return %c: index
}
//CHECK-LABEL:  func.func private @f_sub
//CHECK-SAME:   (%[[A0:[0-9a-zA-Z_\.]+]]: index, %[[A1:[0-9a-zA-Z_\.]+]]: index) -> index {
//CHECK-NEXT:     %[[T1:[0-9a-zA-Z_\.]+]] = index.sub %[[A0]], %[[A1]]
